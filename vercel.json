{
    "version": 2, 
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
    }], 
    "routes": [
        {
            "src": "/",
            "dest": "public/index.html"
        }, 
        {
            "src": "/page2.html",
            "dest": "public/page2.html"
        }, 
        {
            "src": "/css/style.css",
            "dest": "public/css/style.css"
        }
    ]
}